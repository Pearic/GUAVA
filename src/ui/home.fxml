                <Font size="14.0" />
            </font>
        </Label>
        <Label fx:id="usernameLabel" layoutX="27.0" layoutY="211.0">
            <font>
                <Font size="64.0" />
            </font>
        </Label>
      <Button fx:id="viewStorageButton" layoutX="30.0" layoutY="100.0" mnemonicParsing="false" onAction="#viewStorageButtonPressed" text="View storage">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="addFoodButton" layoutX="30.0" layoutY="150.0" mnemonicParsing="false" onAction="#addFoodButtonPressed" text="Add food to storage">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="addSeasoningButton" layoutX="30.0" layoutY="200.0" mnemonicParsing="false" onAction="#addSeasoningButtonPressed" text="Add seasoning to storage">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Button fx:id="removeFoodButton" layoutX="30.0" layoutY="250.0" mnemonicParsing="false" onAction="#removeFoodButtonPressed" text="Remove food from storage">
