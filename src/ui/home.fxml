<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="background" prefHeight="800.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.HomePageController">
    <children>
        <Label fx:id="homeLabel" layoutX="72.0" layoutY="766.0" prefHeight="20.0" prefWidth="40.0" text="Home" textAlignment="CENTER">
